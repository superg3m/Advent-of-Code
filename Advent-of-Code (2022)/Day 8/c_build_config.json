{
    "c_build_version": "0.9",
    "project_name": "AOC_DAY8",
    "github_root": "https://github.com/superg3m",
    "compiler_type": "gcc",
    "std_version": "c11",
    "compiler_warning_level": "all",

    "compiler_disable_warnings": false,
    "debug_with_visual_studio": false,
    "should_rebuild_project_dependencies": false,
    "compiler_treat_warnings_as_errors": true,

    "project_dependencies": ["ckit"],
    "disable_specific_warnings": [""],
    "inject_as_argument": [""],

    "./build_$compiler_type" : {
        "output_name" : "AOC_day8.exe",
        "compile_time_defines" : "",
        "include_paths" : ["../ckit/ckg/Include", "../ckit/Include/Core", "../ckit/Include/Modules"],
        "source_files" : [
            "../source/*.c"
        ],

        "additional_libs" : [
            "../ckit/ckit_build_$compiler_type/ckit.lib"
        ]
    },

    "execute" : ["AOC_day8.exe"]
}

